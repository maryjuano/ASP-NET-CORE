<div class="d-flex justify-content-end form-header">
    <div class="mr-auto p-1">
        <h2>Record Primary Name</h2>
    </div>
    <div class="p-1">
        <h2>Something can be put here..</h2>
    </div>
</div>

<form>
    <div class="card section">
        <div class="card-header" role="tab" id="headingOne">
            <h5 class="mb-0">
                <a data-toggle="collapse" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                    General Information
                </a>
            </h5>
        </div>
        <div id="collapseOne" class="collapse show" role="tabpanel" aria-labelledby="headingOne" data-parent="#accordion">
            <div class="card-body">
                <div class="form-row">
                    <div class="form-group col-md-3">
                        <label for="inputEmail4">Email</label>
                        <input type="email" class="form-control form-control-sm" id="inputEmail4" placeholder="Email">
                    </div>
                    <div class="form-group col-md-3">
                        <label for="inputPassword4">Password</label>
                        <input type="password" class="form-control form-control-sm" id="inputPassword4" placeholder="Password">
                    </div>
                    <div class="form-group col-md-6">
                        <label for="inputAddress2">Address 2</label>
                        <input type="text" class="form-control form-control-sm" id="inputAddress2" placeholder="Apartment, studio, or floor">
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label for="inputAddress">Address</label>
                        <input type="text" class="form-control form-control-sm" id="inputAddress" placeholder="1234 Main St">
                    </div>
                    <div class="form-group col-md-2">
                        <label for="inputCity">City</label>
                        <input type="text" class="form-control form-control-sm" id="inputCity" placeholder="Quezon City">
                    </div>
                    <div class="form-group col-md-2">
                        <label for="inputState">State</label>
                        <select id="inputState" class="form-control form-control-sm">
                            <option selected>Choose...</option>
                            <option>...</option>
                        </select>
                    </div>
                    <div class="form-group col-md-2">
                        <label for="inputZip">Zip</label>
                        <input type="text" class="form-control form-control-sm" id="inputZip" placeholder="1900">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="card section">
        <div class="card-header" role="tab" id="headingTwo">
            <h5 class="mb-0">
                <a data-toggle="collapse" href="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
                    Child Entity Subgrid
                </a>
            </h5>
        </div>
        <div id="collapseTwo" class="collapse" role="tabpanel" aria-labelledby="headingTwo">
            <div class="card-body">
                <table class="table table-sm table-responsive-lg table-bordered table-hover">
                    <thead class="thead-dark">
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">First Name</th>
                            <th scope="col">Last Name</th>
                            <th scope="col">Username</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <th scope="row">1</th>
                            <td>Mark</td>
                            <td>Otto</td>
                            <td>#mdo</td>
                        </tr>
                        <tr>
                            <th scope="row">2</th>
                            <td>Jacob</td>
                            <td>Thornton</td>
                            <td>#fat</td>
                        </tr>
                        <tr>
                            <th scope="row">3</th>
                            <td>Larry</td>
                            <td>the Bird</td>
                            <td>#twitter</td>
                        </tr>
                        <tr>
                            <th scope="row">1</th>
                            <td>Mark</td>
                            <td>Otto</td>
                            <td>#mdo</td>
                        </tr>
                        <tr>
                            <th scope="row">2</th>
                            <td>Jacob</td>
                            <td>Thornton</td>
                            <td>#fat</td>
                        </tr>
                        <tr>
                            <th scope="row">4</th>
                            <td>Larry</td>
                            <td>the Bird</td>
                            <td>#twitter</td>
                        </tr>
                        <tr>
                            <th scope="row">1</th>
                            <td>Mark</td>
                            <td>Otto</td>
                            <td>#mdo</td>
                        </tr>
                        <tr>
                            <th scope="row">2</th>
                            <td>Jacob</td>
                            <td>Thornton</td>
                            <td>#fat</td>
                        </tr>
                        <tr>
                            <th scope="row">5</th>
                            <td>Larry</td>
                            <td>the Bird</td>
                            <td>#twitter</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <div class="card section">
        <div class="card-header" role="tab" id="headingThree">
            <h5 class="mb-0">
                <a data-toggle="collapse" href="#collapseThree" aria-expanded="true" aria-controls="collapseThree">
                    Record Information
                </a>
            </h5>
        </div>
        <div id="collapseThree" class="collapse" role="tabpanel" aria-labelledby="headingThree">
            <div class="card-body">
                <div class="form-row">
                    <div class="form-group col-md-4">
                        <label for="RecordOwner">Record Owner</label>
                        <input type="text" class="form-control form-control-sm" id="RecordOwner" placeholder="John Doe">
                    </div>
                    <div class="form-group col-md-4">
                        <label for="inputPassword4">Owning Dealer</label>
                        <input type="text" class="form-control form-control-sm" id="inputPassword4" placeholder="Citimotors">
                    </div>
                    <div class="form-group col-md-4">
                        <label for="inputAddress2">Created On</label>
                        <input type="text" class="form-control form-control-sm" id="inputAddress2" placeholder="11/1/2017">
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-4 offset-md-4">
                        <label for="inputPassword4">Owning Branch</label>
                        <input type="text" class="form-control form-control-sm" id="inputPassword4" placeholder="Citimotors Greenhills">
                    </div>
                    <div class="form-group col-md-4">
                        <label for="inputAddress2">Modifed On</label>
                        <input type="text" class="form-control form-control-sm" id="inputAddress2" placeholder="11/1/2017">
                    </div>
                </div>
            </div>
        </div>
    </div>


</form>


